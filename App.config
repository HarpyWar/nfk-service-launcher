<?xml version="1.0"?>
<configuration>
    <appSettings>
      <add key="Title" value="Need For Kill - Dedicated Server"/>
      <add key="ServerExeFile" value="server.dat"/>
      <add key="ExeParameters" value="+gowindow +nosound +nfkplanet +game server +exec server +dontsavecfg software"/>
      <add key="LogFile" value="basenfk\realtimelog.txt"/>
      <add key="ProcessorAffinity" value="FF"/><!-- hex mask -->
      <add key="ProcessorPriority" value="3"/><!-- numeric 0-5 -->
      <add key="ServiceName" value="NFK"/>
      <add key="AutoUpdate" value="true"/>
      <add key="AutoUpdateUrl" value="http://nfk.pro2d.ru/files/update/Server/nfkupdate.xml"/>
    </appSettings>
</configuration>